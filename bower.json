{
  "name": "cf-forms",
  "version": "0.4.0",
  "keywords": ["capital-framework", "capital", "forms", "less"],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "url": "http://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/cf-forms",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/cf-forms.git"
  },
  "license": "https://github.com/cfpb/cf-forms/blob/master/TERMS.md",
  "main": "src/forms.less",
  "dependencies": {
    "fj-colors": "git://fake.ghe.domain/flapjack/fj-colors.git",
    "cf-typography": "git://fake.ghe.domain/flapjack/cf-typography.git",
    "font-awesome": "~3.2.1"
  },
  "exportsOverride": {
    "fj-*": {
      "css": "src/*.css",
      "less": "src/*.less",
      "fonts": "src/fonts/*.*"
    },
    "cfpb-font-icons": {
      "css": "src/*.css",
      "fonts": "static/fonts/*.*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
